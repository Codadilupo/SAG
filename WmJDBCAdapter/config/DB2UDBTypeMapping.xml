<?xml version="1.0" encoding="UTF-8"?>
<databaseConfiguration dbVendor="DB2AS400">
	<typeMapping>
		<jdbcType name="SQLXML">
			<javaType name="STRING"/>
			<javaType name="OBJECT"/>
		</jdbcType>
	</typeMapping>
	<!-- <extTypeMapping>

	</extTypeMapping> -->
	<columnTypeMapping>
		<columnType nativeSqlType="BIGINT" jdbcTypeName="BIGINT"  includeColumnSize="false"/>
		<columnType nativeSqlType="INTEGER" jdbcTypeName="INTEGER"  includeColumnSize="false"/>
		<columnType nativeSqlType="SMALLINT" jdbcTypeName="SMALLINT"  includeColumnSize="false"/>
		<columnType nativeSqlType="NUMERIC" jdbcTypeName="NUMERIC" includePrecision="true" />
		<columnType nativeSqlType="DECIMAL" jdbcTypeName="DECIMAL"  includePrecision="true" />
		
		<columnType nativeSqlType="FLOAT" jdbcTypeName="DOUBLE" includeColumnSize="false" />
		<columnType nativeSqlType="REAL" jdbcTypeName="REAL" includeColumnSize="false" />
		<columnType nativeSqlType="VARCHAR" jdbcTypeName="VARCHAR" />
		<columnType nativeSqlType="GRAPHIC" jdbcTypeName="CHAR" resizeFactor="0.5"/>
		<columnType nativeSqlType="VARGRAPHIC" jdbcTypeName="VARCHAR"  resizeFactor="0.5"/>
		
		<columnType nativeSqlType="TIMESTAMP" jdbcTypeName="TIMESTAMP" includeColumnSize="false"/>
		<columnType nativeSqlType="DATE" jdbcTypeName="DATE" includeColumnSize="false" />
		<columnType nativeSqlType="TIME" jdbcTypeName="TIME" includeColumnSize="false" />
		<columnType nativeSqlType="ROWID" jdbcTypeName="BINARY"/>
		<columnType nativeSqlType="DBCLOB" jdbcTypeName="CLOB" includeColumnSize="false" /> 
		<columnType nativeSqlType="XML" jdbcTypeName="SQLXML" includeColumnSize="false"/>
	</columnTypeMapping>
</databaseConfiguration>